    "\n",
    "# Create Fact Tables\n",
    "from pyspark.sql.functions import year, month, dayofmonth, quarter, sum, avg, count, countDistinct, min, max, to_date\n",
    "\n",
    "# FactSales - Sales Transactions\n",
    "print(\"\\n=== CREATING FACT SALES ===\")\n",
    "if 'retail_analytical_dataset' in silver_data:\n",
    "    sales_df = silver_data['retail_analytical_dataset']\n",
    "    \n",
