   "source": [
    "# Configuration and Setup\n",
    "storage_account = \"adls4salesdata\"\n",
    "silver_container = \"transformed-data\"\n",
    "gold_container = \"ready-data\"\n",
    "account_key = ""keygoeshere"\"\n",
    "\n",
    "# Configure storage access\n",
    "spark.conf.set(f\"fs.azure.account.key.{storage_account}.blob.core.windows.net\", account_key)\n",
    "\n",
    "# Configure storage paths\n",
    "silver_base_path = f\"wasbs://{silver_container}@{storage_account}.blob.core.windows.net\"\n",
    "gold_base_path = f\"wasbs://{gold_container}@{storage_account}.blob.core.windows.net\"\n",
    "\n",
    "# Create gold directory structure\n",
    "dbutils.fs.mkdirs(f\"{gold_base_path}/dimensions\")\n",
    "dbutils.fs.mkdirs(f\"{gold_base_path}/facts\") \n",
    "dbutils.fs.mkdirs(f\"{gold_base_path}/enriched_views\")\n",
    "dbutils.fs.mkdirs(f\"{gold_base_path}/business_kpis\")\n",
    "\n",
    "# Configure Spark for optimized performance\n",
    "spark.conf.set(\"spark.sql.adaptive.enabled\", \"true\")\n",
    "spark.conf.set(\"spark.sql.adaptive.coalescePartitions.enabled\", \"true\")\n",
    "spark.conf.set(\"spark.sql.adaptive.skew.enabled\", \"true\")\n",
    "\n",
    "print(\"Gold layer configuration completed successfully!\")\n",
    "\n",
    "# List all files in silver container\n",
    "print(\"=== SILVER LAYER FILES ===\")\n",
    "silver_files = dbutils.fs.ls(silver_base_path)\n",
    "for file_info in silver_files:\n",
    "    print(f\"File: {file_info.name}, Size: {file_info.size}\")\n",
    "\n",
    "# Function to read all parquet files from a directory\n",
    "def read_all_parquet_files(base_path):\n",
    "    \"\"\"Read all parquet files from a directory and return as dictionary of DataFrames\"\"\"\n",
    "    dataframes = {}\n",
    "    try:\n",
    "        files = dbutils.fs.ls(base_path)\n",
    "        for file_info in files:\n",
    "            if file_info.name.endswith('.parquet/') or file_info.name.endswith('.parquet'):\n",
    "                # Extract table name from file path\n",
    "                table_name = file_info.name.replace('.parquet', '').replace('/', '')\n",
    "                if table_name:\n",
    "                    try:\n",
    "                        file_path = f\"{base_path}/{file_info.name}\"\n",
    "                        if file_info.name.endswith('/'):\n",
    "                            file_path = file_path[:-1]\n",
    "                            \n",
    "                        df = spark.read.parquet(file_path)\n",
    "                        dataframes[table_name] = df\n",
    "                        print(f\"✓ Loaded {table_name}: {df.count():,} records, {len(df.columns)} columns\")\n",
    "                        print(f\"  Columns: {df.columns}\")\n",
    "                    except Exception as e:\n",
    "                        print(f\"✗ Error loading {table_name}: {str(e)}\")\n",
    "    except Exception as e:\n",
    "        print(f\"Error accessing directory {base_path}: {str(e)}\")\n",
    "    \n",
    "    return dataframes\n",
    "\n",
    "# Read all silver layer data\n",
    "print(\"\\n=== LOADING SILVER LAYER DATA ===\")\n",
    "silver_data = read_all_parquet_files(silver_base_path)\n",
    "\n",
    "# Display overview of loaded data\n",
    "print(\"\\n=== SILVER DATA OVERVIEW ===\")\n",
    "for table_name, df in silver_data.items():\n",
    "    print(f\"\\n{table_name}: {df.count():,} records, {len(df.columns)} columns\")\n",
    "    df.printSchema()\n",
    "\n",
    "# Data Quality Checks and Validation\n",
    "from pyspark.sql.functions import col, count, when, isnan, sum as spark_sum, lit\n",
    "\n",
    "def data_quality_checks(df, df_name):\n",
    "    \"\"\"Perform comprehensive data quality checks\"\"\"\n",
    "    print(f\"\\n=== DATA QUALITY CHECK: {df_name} ===\")\n",
    "    \n",
    "    # Basic stats\n",
    "    print(f\"Total Records: {df.count():,}\")\n",
    "    print(f\"Total Columns: {len(df.columns)}\")\n",
    "    print(f\"Columns: {df.columns}\")\n",
    "    \n",
    "    # Missing values check\n",
    "    missing_expr = [spark_sum(when(col(c).isNull(), 1).otherwise(0)).alias(c) for c in df.columns]\n",
    "    if missing_expr:\n",
    "        missing_data = df.select(*missing_expr)\n",
    "        print(\"Missing Values per Column:\")\n",
    "        missing_data.show(vertical=True, truncate=False)\n",
    "    \n",
    "    # Duplicate check\n",
    "    duplicate_count = df.count() - df.distinct().count()\n",
    "    print(f\"Duplicate Records: {duplicate_count}\")\n",
    "    \n",
    "    return df\n",
    "\n",
    "# Run data quality checks on all tables\n",
    "print(\"\\n=== RUNNING DATA QUALITY CHECKS ===\")\n",
    "for table_name, df in silver_data.items():\n",
    "    data_quality_checks(df, table_name)\n",
    "\n",
    "# Create Core Dimension Tables\n",
    "from pyspark.sql.functions import md5, concat_ws, current_timestamp, when, lit, col, coalesce\n",
    "\n",
    "# DimCustomer - Enhanced Customer Dimension\n",
    "print(\"\\n=== CREATING DIM CUSTOMER ===\")\n",
    "if 'customers_clean' in silver_data:\n",
    "    customers_df = silver_data['customers_clean']\n",
    "    \n",
    "    # Check actual columns in customers_clean\n",
    "    print(\"Available columns in customers_clean:\")\n",
    "    customers_df.printSchema()\n",
    "    \n",
    "    # Create DimCustomer with available columns\n",
    "    dim_customer = (\n",
    "        customers_df\n",
    "        .withColumn(\"CustomerKey\", md5(concat_ws(\"|\", col(\"CustomerID\"))))\n",
    "        .withColumn(\"CustomerAlternateKey\", col(\"CustomerID\"))\n",
    "        .withColumn(\"FullName\", concat_ws(\" \", col(\"FirstName\"), col(\"LastName\")))\n",
    "        # Create CustomerSegment based on available data or set default\n",
    "        .withColumn(\"CustomerSegment\", \n",
    "                   when(col(\"Country\").isin([\"USA\", \"Canada\"]), \"Domestic\")\n",
    "                   .when(col(\"Country\").isNotNull(), \"International\")\n",
    "                   .otherwise(\"Standard\"))\n",
    "        .withColumn(\"IsActive\", lit(True))\n",
    "        .withColumn(\"ValidFrom\", current_timestamp())\n",
    "        .withColumn(\"ValidTo\", lit(None).cast(\"timestamp\"))\n",
    "        .withColumn(\"Lineage\", lit(\"Silver_to_Gold\"))\n",
    "        .select(\n",
    "            \"CustomerKey\",\n",
    "            \"CustomerAlternateKey\", \n",
    "            \"CustomerID\",\n",
    "            \"FirstName\",\n",
    "            \"LastName\", \n",
    "            \"FullName\",\n",
    "            \"CustomerSegment\",\n",
    "            \"Country\",\n",
    "            \"City\",\n",
    "            \"IsActive\",\n",
    "            \"ValidFrom\",\n",
    "            \"ValidTo\",\n",
    "            \"Lineage\"\n",
    "        )\n",
    "        .distinct()\n",
    "    )\n",
    "    \n",
    "    print(f\"DimCustomer created: {dim_customer.count():,} records\")\n",
    "    dim_customer.show(10)\n",
    "else:\n",
    "    print(\"Customers data not available for DimCustomer creation\")\n",
    "\n",
    "# DimProduct - Product Dimension\n",
    "print(\"\\n=== CREATING DIM PRODUCT ===\")\n",
    "if 'products' in silver_data:\n",
    "    products_df = silver_data['products']\n",
    "    \n",
    "    # Check actual columns in products\n",
    "    print(\"Available columns in products:\")\n",
    "    products_df.printSchema()\n",
    "    \n",
    "    # Check available columns\n",
    "    product_columns = products_df.columns\n",
    "    print(f\"Available product columns: {product_columns}\")\n",
    "    \n",
    "    # Start building dim_product with base columns\n",
    "    dim_product = (\n",
    "        products_df\n",
    "        .withColumn(\"ProductKey\", md5(concat_ws(\"|\", col(\"ProductID\"))))\n",
    "        .withColumn(\"ProductAlternateKey\", col(\"ProductID\"))\n",
    "        .withColumn(\"IsActive\", lit(True))\n",
    "        .withColumn(\"ValidFrom\", current_timestamp())\n",
    "        .withColumn(\"ValidTo\", lit(None).cast(\"timestamp\"))\n",
    "        .withColumn(\"Lineage\", lit(\"Silver_to_Gold\"))\n",
    "    )\n",
    "    \n",
    "    # Handle Category based on available columns\n",
    "    if 'Category' in product_columns:\n",
    "        dim_product = dim_product.withColumn(\"Category\", col(\"Category\"))\n",
    "    elif 'CategoryID' in product_columns:\n",
    "        dim_product = dim_product.withColumn(\"Category\", col(\"CategoryID\"))\n",
    "    else:\n",
    "        dim_product = dim_product.withColumn(\"Category\", lit(\"General\"))\n",
    "    \n",
    "    # Handle SubCategory based on available columns\n",
    "    if 'SubCategory' in product_columns:\n",
    "        dim_product = dim_product.withColumn(\"SubCategory\", col(\"SubCategory\"))\n",
    "    elif 'SubcategoryID' in product_columns:\n",
    "        dim_product = dim_product.withColumn(\"SubCategory\", col(\"SubcategoryID\"))\n",
    "    else:\n",
    "        dim_product = dim_product.withColumn(\"SubCategory\", lit(\"General\"))\n",
    "    \n",
    "    # Handle Price - check if Price exists, if not check for UnitPrice or use CostPrice as reference\n",
    "    if 'Price' in product_columns:\n",
    "        dim_product = dim_product.withColumn(\"Price\", col(\"Price\"))\n",
    "    elif 'UnitPrice' in product_columns:\n",
    "        dim_product = dim_product.withColumn(\"Price\", col(\"UnitPrice\"))\n",
    "    else:\n",
    "        # If no price column, set a default or use CostPrice * 1.2 for markup\n",
    "        dim_product = dim_product.withColumn(\"Price\", lit(0.0))\n",
    "    \n",
    "    # Handle Cost - check if Cost exists\n",
    "    if 'Cost' in product_columns:\n",
    "        dim_product = dim_product.withColumn(\"Cost\", col(\"Cost\"))\n",
    "    elif 'CostPrice' in product_columns:\n",
    "        dim_product = dim_product.withColumn(\"Cost\", col(\"CostPrice\"))\n",
    "    else:\n",
    "        dim_product = dim_product.withColumn(\"Cost\", lit(0.0))\n",
    "    \n",
    "    # Calculate ProfitMargin safely\n",
    "    dim_product = dim_product.withColumn(\n",
    "        \"ProfitMargin\",\n",
    "        when((col(\"Price\") > 0) & (col(\"Cost\") > 0), ((col(\"Price\") - col(\"Cost\")) / col(\"Price\")) * 100)\n",
    "        .otherwise(lit(0.0))\n",
    "    )\n",
    "    \n",
    "    # Build select columns dynamically based on what exists\n",
    "    select_columns = [\n",
    "        \"ProductKey\",\n",
    "        \"ProductAlternateKey\",\n",
    "        \"ProductID\",\n",
    "        \"ProductName\", \n",
    "        \"Category\",\n",
    "        \"SubCategory\",\n",
    "        \"Brand\",\n",
    "        \"Price\",\n",
    "        \"Cost\",\n",
    "        \"ProfitMargin\",\n",
    "        \"IsActive\",\n",
    "        \"ValidFrom\", \n",
    "        \"ValidTo\",\n",
    "        \"Lineage\"\n",
    "    ]\n",
    "    \n",
    "    # Filter to only include columns that exist\n",
    "    available_columns = [col for col in select_columns if col in dim_product.columns]\n",
    "    \n",
    "    dim_product = dim_product.select(*available_columns).distinct()\n",
    "    \n",
    "    print(f\"DimProduct created: {dim_product.count():,} records\")\n",
    "    print(\"DimProduct schema:\")\n",
    "    dim_product.printSchema()\n",
    "    dim_product.show(10)\n",
    "else:\n",
    "    print(\"Products data not available for DimProduct creation\")\n",
    "\n",
    "# DimStore - Store Dimension  \n",
    "print(\"\\n=== CREATING DIM STORE ===\")\n",
    "if 'stores' in silver_data:\n",
    "    stores_df = silver_data['stores']\n",
    "    \n",
    "    # Check actual columns in stores\n",
    "    print(\"Available columns in stores:\")\n",
    "    stores_df.printSchema()\n",
    "    \n",
    "    # Check available columns in stores\n",
    "    store_columns = stores_df.columns\n",
    "    print(f\"Available store columns: {store_columns}\")\n",
    "    \n",
    "    # Start building dim_store with base columns\n",
    "    dim_store = (\n",
    "        stores_df\n",
    "        .withColumn(\"StoreKey\", md5(concat_ws(\"|\", col(\"StoreID\"))))\n",
    "        .withColumn(\"StoreAlternateKey\", col(\"StoreID\"))\n",
    "        .withColumn(\"IsActive\", lit(True))\n",
    "        .withColumn(\"ValidFrom\", current_timestamp())\n",
    "        .withColumn(\"ValidTo\", lit(None).cast(\"timestamp\"))\n",
    "        .withColumn(\"Lineage\", lit(\"Silver_to_Gold\"))\n",
    "    )\n",
    "    \n",
    "    # Handle Country based on available columns\n",
    "    if 'Country' in store_columns:\n",
    "        dim_store = dim_store.withColumn(\"Country\", col(\"Country\"))\n",
    "    else:\n",
    "        dim_store = dim_store.withColumn(\"Country\", lit(\"Unknown\"))\n",
    "    \n",
    "    # Handle Region based on available columns\n",
    "    if 'Region' in store_columns:\n",
    "        dim_store = dim_store.withColumn(\"Region\", col(\"Region\"))\n",
    "    else:\n",
    "        dim_store = dim_store.withColumn(\"Region\", lit(\"Unknown\"))\n",
    "    \n",
    "    # Handle State based on available columns\n",
    "    if 'State' in store_columns:\n",
    "        dim_store = dim_store.withColumn(\"State\", col(\"State\"))\n",
    "    else:\n",
    "        dim_store = dim_store.withColumn(\"State\", lit(\"Unknown\"))\n",
    "    \n",
    "    # Handle City based on available columns\n",
    "    if 'City' in store_columns:\n",
    "        dim_store = dim_store.withColumn(\"City\", col(\"City\"))\n",
    "    else:\n",
    "        dim_store = dim_store.withColumn(\"City\", lit(\"Unknown\"))\n",
    "    \n",
    "    # Build select columns dynamically based on what exists\n",
    "    select_columns = [\n",
    "        \"StoreKey\",\n",
    "        \"StoreAlternateKey\",\n",
    "        \"StoreID\",\n",
    "        \"StoreName\",\n",
    "        \"Region\", \n",
    "        \"Country\",\n",
    "        \"State\",\n",
    "        \"City\",\n",
    "        \"IsActive\",\n",
    "        \"ValidFrom\",\n",
    "        \"ValidTo\", \n",
    "        \"Lineage\"\n",
    "    ]\n",
