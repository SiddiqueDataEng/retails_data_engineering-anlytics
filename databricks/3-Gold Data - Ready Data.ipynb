    "    print(\"Available columns in retail_analytical_dataset:\")\n",
    "    sales_df.printSchema()\n",
    "    \n",
    "    # Check available columns\n",
    "    sales_columns = sales_df.columns\n",
    "    print(f\"Available sales columns: {sales_columns}\")\n",
    "    \n",
    "    # Start building fact_sales with base columns\n",
    "    fact_sales = (\n",
    "        sales_df\n",
    "        .withColumn(\"SalesKey\", md5(concat_ws(\"|\", \n",
    "                    coalesce(col(\"TransactionID\"), lit(\"\")), \n",
    "                    coalesce(col(\"CustomerID\"), lit(\"\")), \n",
    "                    coalesce(col(\"ProductID\"), lit(\"\")))))\n",
    "        .withColumn(\"CustomerKey\", md5(concat_ws(\"|\", coalesce(col(\"CustomerID\"), lit(\"\")))))\n",
    "        .withColumn(\"ProductKey\", md5(concat_ws(\"|\", coalesce(col(\"ProductID\"), lit(\"\")))))\n",
    "        .withColumn(\"StoreKey\", md5(concat_ws(\"|\", coalesce(col(\"StoreID\"), lit(\"\")))))\n",
    "        .withColumn(\"Lineage\", lit(\"Silver_to_Gold\"))\n",
    "    )\n",
    "    \n",
    "    # Handle Date - check for different possible date column names\n",
    "    if 'Date' in sales_columns:\n",
    "        fact_sales = fact_sales.withColumn(\"DateKey\", col(\"Date\").cast(\"date\"))\n",
    "    elif 'TransactionDate' in sales_columns:\n",
    "        fact_sales = fact_sales.withColumn(\"DateKey\", col(\"TransactionDate\").cast(\"date\"))\n",
    "    elif 'SaleDate' in sales_columns:\n",
    "        fact_sales = fact_sales.withColumn(\"DateKey\", col(\"SaleDate\").cast(\"date\"))\n",
    "    else:\n",
    "        # If no date column, use current date as default\n",
    "        fact_sales = fact_sales.withColumn(\"DateKey\", current_timestamp().cast(\"date\"))\n",
    "    \n",
    "    # Extract date components\n",
    "    fact_sales = (\n",
    "        fact_sales\n",
    "        .withColumn(\"Year\", year(\"DateKey\"))\n",
    "        .withColumn(\"Quarter\", quarter(\"DateKey\"))\n",
    "        .withColumn(\"Month\", month(\"DateKey\"))\n",
    "        .withColumn(\"Day\", dayofmonth(\"DateKey\"))\n",
    "    )\n",
    "    \n",
    "    # Handle numeric columns with fallbacks\n",
    "    if 'Quantity' in sales_columns:\n",
    "        fact_sales = fact_sales.withColumn(\"Quantity\", col(\"Quantity\"))\n",
    "    else:\n",
    "        fact_sales = fact_sales.withColumn(\"Quantity\", lit(1))\n",
    "    \n",
    "    if 'UnitPrice' in sales_columns:\n",
    "        fact_sales = fact_sales.withColumn(\"UnitPrice\", col(\"UnitPrice\"))\n",
    "    else:\n",
    "        fact_sales = fact_sales.withColumn(\"UnitPrice\", lit(0.0))\n",
    "    \n",
    "    if 'TotalSales' in sales_columns:\n",
    "        fact_sales = fact_sales.withColumn(\"TotalSales\", col(\"TotalSales\"))\n",
    "    else:\n",
    "        # Calculate TotalSales if not available\n",
    "        fact_sales = fact_sales.withColumn(\"TotalSales\", col(\"Quantity\") * col(\"UnitPrice\"))\n",
    "    \n",
    "    if 'CostPrice' in sales_columns:\n",
    "        fact_sales = fact_sales.withColumn(\"CostPrice\", col(\"CostPrice\"))\n",
    "    else:\n",
    "        fact_sales = fact_sales.withColumn(\"CostPrice\", lit(0.0))\n",
    "    \n",
    "    # Calculate Profit\n",
    "    fact_sales = fact_sales.withColumn(\"Profit\", col(\"TotalSales\") - col(\"CostPrice\"))\n",
    "    \n",
    "    # Build select columns dynamically\n",
    "    select_columns = [\n",
    "        \"SalesKey\",\n",
    "        \"DateKey\",\n",
    "        \"Year\",\n",
    "        \"Quarter\", \n",
    "        \"Month\",\n",
    "        \"Day\",\n",
    "        \"CustomerKey\",\n",
    "        \"ProductKey\", \n",
    "        \"StoreKey\",\n",
    "        \"TransactionID\",\n",
    "        \"Quantity\",\n",
    "        \"UnitPrice\",\n",
    "        \"TotalSales\",\n",
    "        \"CostPrice\",\n",
    "        \"Profit\",\n",
    "        \"Lineage\"\n",
    "    ]\n",
    "    \n",
    "    # Filter to only include columns that exist\n",
    "    available_columns = [col for col in select_columns if col in fact_sales.columns]\n",
    "    \n",
    "    fact_sales = fact_sales.select(*available_columns).distinct()\n",
    "    \n",
    "    print(f\"FactSales created: {fact_sales.count():,} records\")\n",
    "    fact_sales.show(10)\n",
    "else:\n",
    "    print(\"Sales data not available for FactSales creation\")\n",
    "\n",
    "# FactCustomerMetrics - Customer Behavior Facts\n",
    "print(\"\\n=== CREATING FACT CUSTOMER METRICS ===\")\n",
    "if 'retail_analytical_dataset' in silver_data:\n",
    "    sales_df = silver_data['retail_analytical_dataset']\n",
    "    \n",
    "    # Check available columns for date handling\n",
    "    sales_columns = sales_df.columns\n",
    "    \n",
    "    # Determine which date column to use\n",
    "    date_column = None\n",
    "    if 'Date' in sales_columns:\n",
    "        date_column = \"Date\"\n",
    "    elif 'TransactionDate' in sales_columns:\n",
    "        date_column = \"TransactionDate\"\n",
    "    elif 'SaleDate' in sales_columns:\n",
    "        date_column = \"SaleDate\"\n",
    "    else:\n",
    "        # If no date column, we'll need to handle this differently\n",
    "        date_column = None\n",
    "    \n",
    "    # Calculate customer metrics from transaction data\n",
    "    if date_column:\n",
    "        customer_metrics = (\n",
    "            sales_df\n",
    "            .groupBy(\"CustomerID\")\n",
    "            .agg(\n",
    "                count(\"TransactionID\").alias(\"TotalTransactions\"),\n",
    "                sum(\"TotalSales\").alias(\"TotalSpend\"),\n",
    "                sum(\"Quantity\").alias(\"TotalItemsPurchased\"),\n",
    "                avg(\"TotalSales\").alias(\"AvgTransactionValue\"),\n",
    "                countDistinct(\"ProductID\").alias(\"UniqueProductsPurchased\"),\n",
    "                min(date_column).alias(\"FirstPurchaseDate\"),\n",
    "                max(date_column).alias(\"LastPurchaseDate\")\n",
    "            )\n",
    "        )\n",
    "    else:\n",
    "        # If no date column, skip date-related aggregations\n",
    "        customer_metrics = (\n",
    "            sales_df\n",
    "            .groupBy(\"CustomerID\")\n",
    "            .agg(\n",
    "                count(\"TransactionID\").alias(\"TotalTransactions\"),\n",
    "                sum(\"TotalSales\").alias(\"TotalSpend\"),\n",
    "                sum(\"Quantity\").alias(\"TotalItemsPurchased\"),\n",
    "                avg(\"TotalSales\").alias(\"AvgTransactionValue\"),\n",
    "                countDistinct(\"ProductID\").alias(\"UniqueProductsPurchased\")\n",
    "            )\n",
    "            .withColumn(\"FirstPurchaseDate\", lit(None).cast(\"timestamp\"))\n",
    "            .withColumn(\"LastPurchaseDate\", lit(None).cast(\"timestamp\"))\n",
    "        )\n",
    "    \n",
    "    customer_metrics = (\n",
    "        customer_metrics\n",
    "        .withColumn(\"DateKey\", current_timestamp().cast(\"date\"))\n",
    "        .withColumn(\"CustomerKey\", md5(concat_ws(\"|\", col(\"CustomerID\"))))\n",
    "        .withColumn(\"CustomerValueSegment\",\n",
    "                   when(col(\"TotalSpend\") > 10000, \"Platinum\")\n",
    "                   .when(col(\"TotalSpend\") > 5000, \"Gold\")\n",
    "                   .when(col(\"TotalSpend\") > 1000, \"Silver\")\n",
    "                   .otherwise(\"Bronze\"))\n",
    "        .withColumn(\"Lineage\", lit(\"Silver_to_Gold\"))\n",
    "    )\n",
    "    \n",
    "    fact_customer_metrics = customer_metrics.select(\n",
    "        \"DateKey\",\n",
    "        \"CustomerKey\", \n",
    "        \"CustomerID\",\n",
    "        \"TotalTransactions\",\n",
    "        \"TotalSpend\",\n",
    "        \"TotalItemsPurchased\",\n",
    "        \"AvgTransactionValue\", \n",
    "        \"UniqueProductsPurchased\",\n",
    "        \"FirstPurchaseDate\",\n",
    "        \"LastPurchaseDate\",\n",
    "        \"CustomerValueSegment\",\n",
    "        \"Lineage\"\n",
    "    )\n",
    "    \n",
    "    print(f\"FactCustomerMetrics created: {fact_customer_metrics.count():,} records\")\n",
    "    fact_customer_metrics.show(10)\n",
    "else:\n",
    "    print(\"Required data not available for FactCustomerMetrics creation\")\n",
    "\n",
    "# FactProductPerformance - Product Performance Facts\n",
    "print(\"\\n=== CREATING FACT PRODUCT PERFORMANCE ===\")\n",
    "if 'retail_analytical_dataset' in silver_data:\n",
    "    sales_df = silver_data['retail_analytical_dataset']\n",
    "    \n",
    "    product_performance = (\n",
    "        sales_df\n",
    "        .groupBy(\"ProductID\")\n",
    "        .agg(\n",
    "            count(\"TransactionID\").alias(\"TotalSalesTransactions\"),\n",
    "            sum(\"Quantity\").alias(\"TotalUnitsSold\"),\n",
    "            sum(\"TotalSales\").alias(\"TotalRevenue\"),\n",
    "            sum(\"CostPrice\").alias(\"TotalCost\"),\n",
    "            (sum(\"TotalSales\") - sum(\"CostPrice\")).alias(\"TotalProfit\"),\n",
    "            avg(\"TotalSales\").alias(\"AvgSaleValue\"),\n",
    "            countDistinct(\"CustomerID\").alias(\"UniqueCustomers\")\n",
    "        )\n",
    "        .withColumn(\"DateKey\", current_timestamp().cast(\"date\"))\n",
    "        .withColumn(\"ProductKey\", md5(concat_ws(\"|\", col(\"ProductID\"))))\n",
    "        .withColumn(\"ProfitMargin\", \n",
    "                   when(col(\"TotalRevenue\") > 0, (col(\"TotalProfit\") / col(\"TotalRevenue\")) * 100)\n",
    "                   .otherwise(0))\n",
    "        .withColumn(\"Lineage\", lit(\"Silver_to_Gold\"))\n",
    "    )\n",
    "    \n",
    "    fact_product_performance = product_performance.select(\n",
    "        \"DateKey\",\n",
    "        \"ProductKey\",\n",
    "        \"ProductID\",\n",
    "        \"TotalSalesTransactions\", \n",
    "        \"TotalUnitsSold\",\n",
    "        \"TotalRevenue\",\n",
    "        \"TotalCost\",\n",
    "        \"TotalProfit\",\n",
    "        \"AvgSaleValue\",\n",
    "        \"ProfitMargin\",\n",
    "        \"UniqueCustomers\",\n",
    "        \"Lineage\"\n",
    "    )\n",
    "    \n",
    "    print(f\"FactProductPerformance created: {fact_product_performance.count():,} records\")\n",
    "    fact_product_performance.show(10)\n",
    "else:\n",
    "    print(\"Required data not available for FactProductPerformance creation\")\n",
    "\n",
    "# Save Gold Layer Data\n",
    "print(\"\\n=== SAVING GOLD LAYER DATA ===\")\n",
    "\n",
    "# Save Dimension Tables\n",
    "if 'dim_customer' in locals():\n",
    "    dim_customer.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/dimensions/dim_customer\")\n",
    "    print(\"✓ DimCustomer saved to gold layer\")\n",
    "\n",
    "if 'dim_product' in locals():\n",
    "    dim_product.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/dimensions/dim_product\")\n",
    "    print(\"✓ DimProduct saved to gold layer\")\n",
    "\n",
    "if 'dim_store' in locals():\n",
    "    dim_store.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/dimensions/dim_store\")\n",
    "    print(\"✓ DimStore saved to gold layer\")\n",
    "\n",
    "# Save Fact Tables\n",
    "if 'fact_sales' in locals():\n",
    "    fact_sales.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/facts/fact_sales\")\n",
    "    print(\"✓ FactSales saved to gold layer\")\n",
    "\n",
    "if 'fact_customer_metrics' in locals():\n",
    "    fact_customer_metrics.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/facts/fact_customer_metrics\")\n",
    "    print(\"✓ FactCustomerMetrics saved to gold layer\")\n",
    "\n",
    "if 'fact_product_performance' in locals():\n",
    "    fact_product_performance.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/facts/fact_product_performance\")\n",
    "    print(\"✓ FactProductPerformance saved to gold layer\")\n",
    "\n",
    "# Create and Save Enriched Views\n",
    "print(\"\\n=== CREATING ENRICHED VIEWS ===\")\n",
    "\n",
    "# Customer 360 View\n",
    "if all(table in locals() for table in ['fact_customer_metrics', 'dim_customer']):\n",
    "    customer_360_view = (\n",
    "        fact_customer_metrics.alias(\"metrics\")\n",
    "        .join(dim_customer.alias(\"cust\"), \"CustomerKey\", \"inner\")\n",
    "        .select(\n",
    "            \"cust.CustomerKey\",\n",
    "            \"cust.CustomerAlternateKey\",\n",
    "            \"cust.FullName\",\n",
    "            \"cust.CustomerSegment\",\n",
    "            \"cust.Country\",\n",
    "            \"metrics.TotalTransactions\",\n",
    "            \"metrics.TotalSpend\",\n",
    "            \"metrics.AvgTransactionValue\",\n",
    "            \"metrics.UniqueProductsPurchased\",\n",
    "            \"metrics.CustomerValueSegment\",\n",
    "            \"metrics.FirstPurchaseDate\",\n",
    "            \"metrics.LastPurchaseDate\"\n",
    "        )\n",
    "    )\n",
    "    \n",
    "    customer_360_view.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/enriched_views/customer_360_view\")\n",
    "    print(\"✓ Customer360View saved\")\n",
    "\n",
    "# Product Performance View\n",
    "if all(table in locals() for table in ['fact_product_performance', 'dim_product']):\n",
    "    product_performance_view = (\n",
    "        fact_product_performance.alias(\"perf\")\n",
    "        .join(dim_product.alias(\"prod\"), \"ProductKey\", \"inner\")\n",
    "        .select(\n",
    "            \"prod.ProductKey\",\n",
    "            \"prod.ProductName\",\n",
    "            \"prod.Category\",\n",
    "            \"prod.Brand\",\n",
    "            \"perf.TotalUnitsSold\",\n",
    "            \"perf.TotalRevenue\",\n",
    "            \"perf.TotalProfit\",\n",
    "            \"perf.ProfitMargin\",\n",
    "            \"perf.UniqueCustomers\"\n",
    "        )\n",
    "    )\n",
    "    \n",
    "    product_performance_view.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/enriched_views/product_performance_view\")\n",
    "    print(\"✓ ProductPerformanceView saved\")\n",
    "\n",
    "# Sales Performance Dashboard\n",
    "if all(table in locals() for table in ['fact_sales', 'dim_customer', 'dim_product', 'dim_store']):\n",
    "    sales_dashboard = (\n",
    "        fact_sales.alias(\"sales\")\n",
    "        .join(dim_customer.alias(\"cust\"), \"CustomerKey\", \"left\")\n",
    "        .join(dim_product.alias(\"prod\"), \"ProductKey\", \"left\")\n",
    "        .join(dim_store.alias(\"store\"), \"StoreKey\", \"left\")\n",
    "        .select(\n",
    "            \"sales.DateKey\",\n",
    "            \"sales.Year\",\n",
    "            \"sales.Month\",\n",
    "            \"cust.FullName\",\n",
    "            \"cust.CustomerSegment\",\n",
    "            \"prod.ProductName\",\n",
    "            \"prod.Category\",\n",
    "            \"store.StoreName\",\n",
    "            \"store.Region\",\n",
    "            \"sales.Quantity\",\n",
    "            \"sales.TotalSales\",\n",
    "            \"sales.Profit\"\n",
    "        )\n",
    "    )\n",
    "    \n",
    "    sales_dashboard.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/enriched_views/sales_dashboard\")\n",
    "    print(\"✓ SalesDashboard saved\")\n",
    "\n",
    "# Create Business KPIs\n",
    "print(\"\\n=== CREATING BUSINESS KPIs ===\")\n",
    "\n",
    "# Monthly Revenue KPI\n",
    "if 'fact_sales' in locals():\n",
    "    monthly_revenue_kpi = (\n",
    "        fact_sales\n",
    "        .groupBy(\"Year\", \"Month\")\n",
    "        .agg(\n",
    "            sum(\"TotalSales\").alias(\"MonthlyRevenue\"),\n",
    "            sum(\"Profit\").alias(\"MonthlyProfit\"),\n",
    "            count(\"SalesKey\").alias(\"TransactionCount\"),\n",
    "            countDistinct(\"CustomerKey\").alias(\"UniqueCustomers\")\n",
    "        )\n",
    "        .withColumn(\"AvgTransactionValue\", col(\"MonthlyRevenue\") / col(\"TransactionCount\"))\n",
    "        .withColumn(\"ProfitMargin\", (col(\"MonthlyProfit\") / col(\"MonthlyRevenue\")) * 100)\n",
    "        .orderBy(\"Year\", \"Month\")\n",
    "    )\n",
    "    \n",
    "    monthly_revenue_kpi.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/business_kpis/monthly_revenue_kpi\")\n",
    "    print(\"✓ MonthlyRevenueKPI saved\")\n",
    "\n",
    "# Customer Segmentation KPI\n",
    "if 'fact_customer_metrics' in locals():\n",
    "    customer_segmentation_kpi = (\n",
    "        fact_customer_metrics\n",
    "        .groupBy(\"CustomerValueSegment\")\n",
    "        .agg(\n",
    "            count(\"CustomerKey\").alias(\"CustomerCount\"),\n",
    "            sum(\"TotalSpend\").alias(\"SegmentRevenue\"),\n",
    "            avg(\"TotalSpend\").alias(\"AvgCustomerValue\"),\n",
    "            avg(\"TotalTransactions\").alias(\"AvgTransactionsPerCustomer\")\n",
    "        )\n",
    "        .orderBy(col(\"SegmentRevenue\").desc())\n",
    "    )\n",
    "    \n",
    "    customer_segmentation_kpi.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/business_kpis/customer_segmentation_kpi\")\n",
    "    print(\"✓ CustomerSegmentationKPI saved\")\n",
    "\n",
    "# Product Category Performance KPI\n",
    "if 'dim_product' in locals() and 'fact_product_performance' in locals():\n",
    "    category_performance_kpi = (\n",
    "        dim_product.alias(\"dim\")\n",
    "        .join(fact_product_performance.alias(\"fact\"), \"ProductKey\")\n",
    "        .groupBy(\"dim.Category\")\n",
    "        .agg(\n",
    "            sum(\"fact.TotalRevenue\").alias(\"CategoryRevenue\"),\n",
    "            sum(\"fact.TotalProfit\").alias(\"CategoryProfit\"),\n",
    "            sum(\"fact.TotalUnitsSold\").alias(\"TotalUnitsSold\"),\n",
    "            countDistinct(\"dim.ProductKey\").alias(\"ProductCount\")\n",
    "        )\n",
    "        .withColumn(\"ProfitMargin\", \n",
    "                   when(col(\"CategoryRevenue\") > 0, (col(\"CategoryProfit\") / col(\"CategoryRevenue\")) * 100)\n",
    "                   .otherwise(0))\n",
    "        .withColumn(\"AvgRevenuePerProduct\", \n",
    "                   when(col(\"ProductCount\") > 0, col(\"CategoryRevenue\") / col(\"ProductCount\"))\n",
    "                   .otherwise(0))\n",
    "        .orderBy(col(\"CategoryRevenue\").desc())\n",
    "    )\n",
    "    \n",
    "    category_performance_kpi.write.mode(\"overwrite\").parquet(f\"{gold_base_path}/business_kpis/category_performance_kpi\")\n",
    "    print(\"✓ CategoryPerformanceKPI saved\")\n",
    "\n",
    "# Final Summary\n",
    "print(\"\\n=== GOLD LAYER CREATION COMPLETE ===\")\n",
    "print(\"Gold layer data has been successfully created and saved to:\")\n",
    "print(f\"Container: {gold_container}\")\n",
    "print(f\"URL: https://{storage_account}.blob.core.windows.net/{gold_container}\")\n",
    "\n",
    "# List created files in gold layer\n",
    "print(\"\\n=== GOLD LAYER FILES ===\")\n",
    "try:\n",
